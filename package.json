    "prettier:format": "prettier --write ./src/**/*.{js,ts,css}"
  },
  "dependencies": {
    "@astrojs/tailwind": "^4.0.0",
    "astro": "^2.7.2",
    "chalk": "^5.3.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "prettier": "^3.0.1",
    "prettier-plugin-astro": "^0.11.0"
  }
